 {
    "name": "Learnify",
    "description" : "Every new tab is a new learning experience!",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
      "default_icon": "icon.png"
    },

    "icons" : {
    	"128" : "icon.png"
    },
     "background": {
    "scripts": ["js/background.js"]
  },
    "chrome_url_overrides" : {
    "newtab": "learntab.html"
  },
  "permissions" : [
  "activeTab",
  "tabs",
  "storage",
  "http://*/*",
  "https://*/*",
  "https://ajax.googleapis.com/"

]

}